{
    "name": "cdnbase",
    "displayName": "cdnbase",
    "description": "Extension with mass amount of CDN links and snippets needed for easy imports",
    "version": "0.1.2",
    "publisher": "sampath9989",
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.73.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/codevedadotorg/cdnbase"
    },
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "php",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "ejs",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "blade",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
